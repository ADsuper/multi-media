<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <!--android:shape="rectangle" 设置形状为矩形-->

    <!--设置圆角弧度-->
    <corners
        android:radius="6dp"/>
    
    <!--设置填充色-->
    <solid
        android:color="@color/white"/>

    <!--设置描边宽度和颜色-->
    <stroke
        android:width="2dp"
        android:color="@color/white"/>
</shape>